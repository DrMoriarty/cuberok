####### Install

install_target: first FORCE
	@$(CHK_DIR_EXISTS) $(INSTALL_ROOT)/usr/bin/ || $(MKDIR) $(INSTALL_ROOT)/usr/bin/ 
	-$(INSTALL_PROGRAM) "unix/$(QMAKE_TARGET)" "$(INSTALL_ROOT)/usr/bin/$(QMAKE_TARGET)"
	-strip "$(INSTALL_ROOT)/usr/bin/$(QMAKE_TARGET)"

uninstall_target:  FORCE
	-$(DEL_FILE) "$(INSTALL_ROOT)/usr/bin/$(QMAKE_TARGET)"
	-$(DEL_DIR) $(INSTALL_ROOT)/usr/bin/ 


install_documentation: first FORCE
	@$(CHK_DIR_EXISTS) $(INSTALL_ROOT)/usr/share/doc/cuberok/ || $(MKDIR) $(INSTALL_ROOT)/usr/share/doc/cuberok/ 
	-$(INSTALL_FILE) /home/vasya/cuberok-0.0.1/README $(INSTALL_ROOT)/usr/share/doc/cuberok/
	-$(INSTALL_FILE) /home/vasya/cuberok-0.0.1/ChangeLog $(INSTALL_ROOT)/usr/share/doc/cuberok/


uninstall_documentation:  FORCE
	-$(DEL_FILE) -r $(INSTALL_ROOT)/usr/share/doc/cuberok/README 
	 -$(DEL_FILE) -r $(INSTALL_ROOT)/usr/share/doc/cuberok/ChangeLog
	-$(DEL_DIR) $(INSTALL_ROOT)/usr/share/doc/cuberok/ 


install_locale: first FORCE
	@$(CHK_DIR_EXISTS) $(INSTALL_ROOT)/usr/share/cuberok/locale/ || $(MKDIR) $(INSTALL_ROOT)/usr/share/cuberok/locale/ 
	-$(INSTALL_FILE) /home/vasya/cuberok-0.0.1/cuberok_ru.qm $(INSTALL_ROOT)/usr/share/cuberok/locale/


uninstall_locale:  FORCE
	-$(DEL_FILE) -r $(INSTALL_ROOT)/usr/share/cuberok/locale/cuberok_ru.qm
	-$(DEL_DIR) $(INSTALL_ROOT)/usr/share/cuberok/locale/ 


install:  install_target install_documentation install_locale  FORCE

uninstall: uninstall_target uninstall_documentation uninstall_locale   FORCE

FORCE:

